<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Testing">

    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register Account"
        android:id="@+id/account_register"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get account"
        android:id="@+id/get_account"
        android:layout_below="@+id/account_register"
        android:layout_alignRight="@+id/account_register"
        android:layout_alignEnd="@+id/account_register"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get id"
        android:id="@+id/get_id"
        android:layout_below="@+id/get_account"
        android:layout_alignRight="@+id/get_account"
        android:layout_alignEnd="@+id/get_account"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Auth"
        android:id="@+id/auth"
        android:layout_below="@+id/get_id"
        android:layout_alignRight="@+id/get_id"
        android:layout_alignEnd="@+id/get_id"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Child register"
        android:id="@+id/child_register"
        android:layout_alignTop="@+id/account_register"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get child"
        android:id="@+id/get_child"
        android:layout_above="@+id/get_id"
        android:layout_alignLeft="@+id/child_register"
        android:layout_alignStart="@+id/child_register"
        android:layout_alignRight="@+id/child_register"
        android:layout_alignEnd="@+id/child_register" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get all"
        android:id="@+id/get_all"
        android:layout_alignTop="@+id/get_id"
        android:layout_alignRight="@+id/child_register"
        android:layout_alignEnd="@+id/child_register"
        android:layout_alignLeft="@+id/get_child"
        android:layout_alignStart="@+id/get_child" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Init Set"
        android:id="@+id/init_set"
        android:layout_alignTop="@+id/child_register"
        android:layout_toRightOf="@+id/child_register"
        android:layout_marginLeft="62dp"
        android:layout_marginStart="62dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Track set"
        android:id="@+id/track_set"
        android:layout_above="@+id/get_all"
        android:layout_alignLeft="@+id/init_set"
        android:layout_alignStart="@+id/init_set"
        android:layout_alignRight="@+id/init_set"
        android:layout_alignEnd="@+id/init_set" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Child report"
        android:id="@+id/child_report"
        android:layout_above="@+id/auth"
        android:layout_alignRight="@+id/track_set"
        android:layout_alignEnd="@+id/track_set"
        android:layout_alignLeft="@+id/track_set"
        android:layout_alignStart="@+id/track_set" />

</RelativeLayout>
